<div class="row-fluid">
  <div class="span12">
    <div class="box">
      <div class="tab-header">
        Gallery
      </div>

      <div class="padded">
        <div id="gallery" data-toggle="modal-gallery" data-target="#modal-gallery">
          <% (1..35).each do |n| %>
              <a title="Picture <%= n %>" rel="gallery" href="../..//images/gallery/large/img<%= n %>.jpg"><img class="img-polaroid" style="margin: 3px;" src="../../images/gallery/thumbs/img<%= n %>.jpg" /></a>
              <%#= link_to image_tag("gallery/thumbs/img#{n}.jpg",class: "img-polaroid", style: "margin: 3px;"), "../../images/gallery/large/img#{n}.jpg", title: "Picture #{n}", rel: "gallery" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>